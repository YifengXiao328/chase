variable A integer;
constant Z integer 2;
constant W boolean true;

Contract C_1:
    variable B boolean;

    Assumptions:
        [](A=2);
    Guarantees:
        [](A=2 -> X(B));

Contract C_2:
    variable C boolean;

    Assumptions:
        [](A=1);
    Guarantees:
        [](A=1 -> X(~C));

Contract C_3:
    Assumptions:
        Z=1;
    Guarantees:
        ~W /\ Z=2;